@model List<GETAF.Models.Entities.GrupoUsuario>

@{
    bool mostrarAcoes = ViewBag.IdUsuarioLogado == ViewBag.CriadorId;
}

    @foreach (var usuario in Model) {
        <tr>
            <td><a href="#">@usuario.Usuario.Nome</a></td>
            <td>Membro</td>
            @if (mostrarAcoes) {
                <td><a class="material-symbols-outlined" onclick="showModal('@usuario.Usuario.Nome',@usuario.UsuarioId)">delete_forever</a></td>
            }
        </tr>
    }

